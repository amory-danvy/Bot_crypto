# Binance API Credentials
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_secret_key_here

# Discord Webhook
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here

# Trading Mode
# Set to 'testnet' for paper trading, 'live' for real trading, 'dry_run' for simulation
TRADING_MODE=dry_run

# Binance Testnet API (optional, for paper trading)
BINANCE_TESTNET_API_KEY=your_testnet_api_key_here
BINANCE_TESTNET_API_SECRET=your_testnet_secret_key_here